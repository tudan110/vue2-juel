{
  "name": "vue2-juel",
  "version": "1.0.1",
  "description": "支持 Vue 2 的 JUEL 表达式生成组件",
  "main": "lib/vue2-juel.umd.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "lib": "vue-cli-service build --target lib --name vue2-juel --dest lib packages/index.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tudan110/vue2-juel.git"
  },
  "keywords": [
    "vue2-juel"
  ],
  "author": "tudan110",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tudan110/vue2-juel/issues"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "element-ui": "2.15.14",
    "lodash": "^4.17.21",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
